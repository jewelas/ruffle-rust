{
    "manifest_version": 2,
    "name": "Ruffle",
    "version": "0.1.0",
    "description": "Puts Flash back on the web where it belongs.",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["dist/ruffle.js"]
        }
    ],
    "web_accessible_resources": ["dist/*"]
}